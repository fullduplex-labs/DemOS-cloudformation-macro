ECS_CLUSTER={{ DemOS.ClusterEcs }}
ECS_RESERVED_PORTS=[22]
ECS_UPDATES_ENABLED=true
ECS_POLL_METRICS=true
ECS_POLLING_METRICS_WAIT_DURATION=30s
# ECS_RESERVED_MEMORY=256
ECS_DISABLE_PRIVILEGED=false
ECS_ENGINE_TASK_CLEANUP_WAIT_DURATION=30m
ECS_ENABLE_TASK_IAM_ROLE=true
ECS_ENABLE_TASK_IAM_ROLE_NETWORK_HOST=true
ECS_DISABLE_IMAGE_CLEANUP=false
ECS_IMAGE_CLEANUP_INTERVAL=30m
ECS_IMAGE_PULL_BEHAVIOR=always
ECS_ENABLE_TASK_ENI=true
ECS_AWSVPC_BLOCK_IMDS=false
# ECS_AWSVPC_ADDITIONAL_LOCAL_ROUTES=["192.168.0.0/16"]
ECS_ENABLE_CONTAINER_METADATA=true
ECS_ENABLE_TASK_CPU_MEM_LIMIT=false
ECS_CONTAINER_INSTANCE_PROPAGATE_TAGS_FROM=ec2_instance
ECS_ENABLE_UNTRACKED_IMAGE_CLEANUP=true
ECS_LOG_OUTPUT_FORMAT=json
ECS_PULL_DEPENDENT_CONTAINERS_UPFRONT=true
ECS_SKIP_LOCALHOST_TRAFFIC_FILTER=false
